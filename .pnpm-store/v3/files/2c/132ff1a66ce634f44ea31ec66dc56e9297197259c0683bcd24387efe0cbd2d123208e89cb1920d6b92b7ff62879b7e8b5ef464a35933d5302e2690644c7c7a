export type PositionComponentFor_HorizontalPositionKeyword =
  | {|
      type: "center",
    |}
  | {|
      type: "length",
      value: DimensionPercentageFor_LengthValue,
    |}
  | {|
      /**
       * Offset from the side.
       */
      offset?: DimensionPercentageFor_LengthValue | null,

      /**
       * A side keyword.
       */
      side: HorizontalPositionKeyword,
      type: "side",
    |};
