export type SelectorComponent =
  | {|
      type: "combinator",
      value: Combinator,
    |}
  | {|
      type: "universal",
    |}
  | (
      | {|
          type: "namespace",
          kind: "none",
        |}
      | {|
          type: "namespace",
          kind: "any",
        |}
      | {|
          type: "namespace",
          kind: "named",
          prefix: string,
        |}
    )
  | {|
      name: string,
      type: "type",
    |}
  | {|
      name: string,
      type: "id",
    |}
  | {|
      name: string,
      type: "class",
    |}
  | {|
      name: string,
      namespace?: NamespaceConstraint | null,
      operation?: AttrOperation | null,
      type: "attribute",
    |}
  | {|
      ...{|
        type: "pseudo-class",
      |},
      ...TSPseudoClass | PseudoClass,
    |}
  | {|
      ...{|
        type: "pseudo-element",
      |},
      ...BuiltinPseudoElement | PseudoElement,
    |}
  | {|
      type: "nesting",
    |};
